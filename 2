#include <stdio.h>
#include <stdlib.h>

int main()
{
	float a, b, c;
	printf("Enter a\n a=");
	scanf_s("%g", &a);
	printf("\nEnter b\n b=");
	scanf_s("%g", &b);
	printf("\nEnter c\n c=");
	scanf_s("%g", &c);
	if (a == b && b == c) printf("Is equilateral");
	else if (a == b || b == c || a==c) printf("Is isoceler");
	else printf("Is not isoceler");

	return 2;

}
